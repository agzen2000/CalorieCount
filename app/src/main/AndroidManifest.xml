<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.games.akash.caloriecount">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fire"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main_menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Logging" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".Settings" />
        <activity android:name=".History" />
        <activity android:name=".MealInfo" />
        <activity android:name=".CalorieCounter" />
        <activity android:name=".DayMeals"></activity>
    </application>

</manifest>